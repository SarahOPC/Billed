{
  "scripts": {
    "test": "jest --coverage --noStackTrace --silent"
  },
  "jest": {
    "verbose": false,
    "testEnvironmentOptions": {
      "url": "http://localhost"
    },
    "setupFiles": [
      "./setup-jest.js"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/app/**",
      "!**/assets/**",
      "!**/external/**",
      "!**/fixtures/**",
      "!**/lcov-report/**"
    ]
  },
  "type": "module",
  "dependencies": {
    "express": "^4.17.1",
    "global": "^4.4.0",
    "jquery": "^3.5.1",
    "jsdoc": "^4.0.2",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.21.4",
    "@testing-library/dom": "^9.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^14.4.3",
    "babel-jest": "^29.5.0",
    "jest": "^29.5.0",
    "jest-cli": "^29.5.0",
    "jest-environment-jsdom": "^29.0.1",
    "jest-html-reporter": "^3.7.1"
  }
}
